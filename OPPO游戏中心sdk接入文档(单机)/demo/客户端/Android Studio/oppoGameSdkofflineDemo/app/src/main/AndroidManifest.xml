<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.nearme.offlinesdk.demo">

    <application
        android:name="com.nearme.offlinesdk.demo.MyApplication"
        tools:replace="android:allowBackup"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name="com.nearme.offlinesdk.demo.DemoActivity"
            android:label="@string/app_name"
            android:screenOrientation="landscape"
            android:configChanges="keyboardHidden|orientation|screenSize">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />

            </intent-filter>
        </activity>
        <uses-library android:name="org.apache.http.legacy" android:required="false" />
        <meta-data android:name="debug_mode" android:value="true" />   <!--  日志开关，发布时候设置为false -->
        <meta-data android:name="is_offline_game" android:value="true" />  <!-- true:单机游戏   false:网游 -->
        <meta-data android:name="app_key" android:value="5hHDqypivkW0c4K4SWS88Sw08" /> <!--  appKey  -->
        <meta-data android:name="android.max_aspect"  android:value="2.4" />
    </application>


</manifest>